// prisma/schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_POSTGRES") // Aquí usa la URL de conexión a tu base de datos
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}